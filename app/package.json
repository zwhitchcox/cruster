{
  "name": "cruster",
  "version": "0.1.0",
  "private": true,
  "author": "Zane Hitchcox <zwhitchcox@gmail.com>",
  "npmRebuild": false,
  "main": "electron/main.js",
  "homepage": "./",
  "description": "Create/Manage/Destroy Kubernetes Cluster on Raspberry Pis very easily",
  "license": "LICENSE.md",
  "dependencies": {
    "electron-squirrel-startup": "^1.0.0",
    "fs-extra": "^9.0.1",
    "node-fetch": "^2.6.0",
    "node-pty": "^0.9.0",
    "node-ssdp": "^4.0.0",
    "node-ssh": "^11.0.0",
    "split2": "^3.2.1",
    "ssh2": "^0.8.9",
    "unzipper": "^0.10.11",
    "uuid": "^8.3.0"
  },
  "scripts": {
    "start-react": "react-scripts start",
    "build-react": "react-scripts build",
    "build": "bash _scripts/build.sh",
    "start-electron": "wait-on tcp:3000 && electron . --development",
    "dev": "concurrently -k \"npm:start-react\" \"npm:start-electron\"",
    "test": "react-app-rewired test",
    "eject": "react-app-rewired eject",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "deploy": "bash _scripts/deploy.sh"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/jest": "^26.0.9",
    "@types/node": "^14.0.27",
    "@types/react": "^16.9.44",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.5",
    "concurrently": "^5.3.0",
    "electron": "^9.1.2",
    "electron-builder": "^22.8.0",
    "electron-rebuild": "^2.0.1",
    "electron-wix-msi": "^3.0.0",
    "node-sass": "^7.0.0",
    "react": "^16.13.1",
    "react-app-rewired": "^2.1.6",
    "react-dom": "^16.13.1",
    "react-ga": "^3.1.2",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^3.4.3",
    "typescript": "^3.9.7",
    "wait-on": "^5.1.0",
    "xterm": "^4.8.1",
    "xterm-addon-attach": "^0.6.0",
    "xterm-addon-web-links": "^0.4.0"
  },
  "optionalDependencies": {
    "balena-image-fs": "^7.0.1"
  }
}
